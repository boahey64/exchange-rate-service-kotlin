{
  "name": "exchange-rate-service-kotlin",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.19.0",
    "parcel-bundler": "^1.12.4",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-redux": "^7.1.0",
    "react-scripts": "3.3.0",
    "redux": "^4.0.4",
    "redux-thunk": "^2.3.0"
  },
  "scripts": {
    "bundle": "parcel build src/main/react/index.js --detailed-report --no-cache --out-dir build/parcel/static/exchange-rate-service-kotlin/dist --out-file bundle-react --public-url /exchange-rate-service-kotlin/dist",
    "watch": "CHOKIDAR_USEPOLLING=true parcel watch src/main/react/index.js --out-dir build/parcel/static/exchange-rate-service-kotlin/dist --out-file bundle-react --public-url /exchange-rate-service-kotlin/dist",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "wiremock": "wiremock --port 18081 --root-dir ./mock"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.8.7",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "expect": "^24.9.0",
    "jest-enzyme": "^7.1.1",
    "moxios": "^0.4.0",
    "redux-mock-store": "^1.5.3",
    "wiremock-standalone": "^2.25.0-1"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env"
      ]
    ],
    "plugins": [
      "@babel/plugin-transform-runtime"
    ]
  }
}
